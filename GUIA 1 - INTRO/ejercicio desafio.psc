// INTERCAMBIAR LAS DOS VARIABLES NUMERICAS INGRESADAS PÓR EL USUARIO.

Algoritmo EjercicioDesafio
	Definir var1, var2, var3 Como entero
	Escribir "Ingrese var1"; Leer var1
	Escribir "Ingrese var2"; Leer var2
	var3 = var1
	var1 = var2
	var2 = var3
	Escribir "var1 es ",var1,""
	Escribir "var2 es ",var2,""
FinAlgoritmo
